{
  "python.formatting.provider": "black",
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Path": "./.venv/bin/flake8",
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "python.linting.enabled": true,
  "cSpell.words": [
    "autofmt",
    "badnights",
    "Datapoints",
    "dtype",
    "figsize",
    "fontsize",
    "fwhm",
    "internight",
    "matplotlib",
    "mdates",
    "mysecretpassword",
    "psycopg",
    "pyplot",
    "savetxt",
    "xaxis",
    "xlabel",
    "yaxis",
    "ylabel"
  ]
}
